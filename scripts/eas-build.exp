#!/usr/bin/expect -f
set timeout 300

spawn eas build --profile preview --platform android

expect {
    "Generate a new Android Keystore?" {
        send "y\r"
        exp_continue
    }
    "Would you like to" {
        send "y\r"
        exp_continue
    }
    "?" {
        send "y\r"
        exp_continue
    }
    eof
}

catch wait result
exit [lindex $result 3]
